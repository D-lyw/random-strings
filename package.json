{
  "name": "@dlyw/random-string",
  "version": "0.1.2",
  "description": "get a random string by user format",
  "main": "lib/rs.js",
  "module": "lib/rs.es.js",
  "scripts": {
    "lint": "eslint src --fix",
    "lint:create": "eslint --init",
    "dev": "rollup -c build/rollup.config.dev.js -w",
    "build:cjs": "rollup -c build/rollup.config.cjs.js",
    "build:iife": "rollup -c build/rollup.config.iife.js",
    "build:es": "rollup -c build/rollup.config.es.js",
    "build": "npm run build:cjs && npm run build:iife && npm run build:es && rollup -c build/rollup.config.umd.js",
    "clean": "rm -rf dist/* && rm -rf lib/*"
  },
  "keywords": [
    "random",
    "string",
    "strings",
    "random-string",
    "typescript"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/D-lyw/random-strings"
  },
  "author": "D-lyw",
  "license": "MIT",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^2.3.1",
    "@typescript-eslint/parser": "^2.3.1",
    "cross-env": "^6.0.0",
    "eslint": "^6.4.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.14.3",
    "rollup": "^1.21.4",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-eslint": "^7.0.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-serve": "^1.0.1",
    "rollup-plugin-strip": "^1.2.2",
    "rollup-plugin-terser": "^5.1.2",
    "rollup-plugin-typescript": "^1.0.1",
    "rollup-plugin-uglify": "^6.0.3",
    "rollup-watch": "^4.3.1",
    "tester": "^1.4.4",
    "tslib": "^1.10.0",
    "typescript": "^3.6.3",
    "typescript-eslint-parser": "^22.0.0"
  },
  "dependencies": {}
}
